{
  "version": "1.0.0",
  "buildOptions": {
    "compilerName": "fsc",
    "debugType": "portable",
    "emitEntryPoint": false,
    "compile": {
      "includeFiles": [
        "../../FSharp.Data.GraphQL.Shared/Extensions.fs",
        "../../FSharp.Data.GraphQL.Shared/Prolog.fs",
        "../../FSharp.Data.GraphQL.Shared/Ast.fs",
        "../../FSharp.Data.GraphQL.Shared/AsyncVal.fs",
        "../../FSharp.Data.GraphQL.Shared/TypeSystem.fs",
        "../../FSharp.Data.GraphQL.Shared/Validation.fs",
        "../../FSharp.Data.GraphQL.Shared/Introspection.fs",
        "../../FSharp.Data.GraphQL.Shared/Parser.fs"
      ]
    },
    "define": [
      "DOTNETCORE",
      "TRACE"
    ],
    "nowarn": [
      "44",
      "2003"
    ],
    "xmlDoc": true,
    "delaySign": true,
    "warningsAsErrors": false,
    "additionalArguments": [
      "--fullpaths",
      "--flaterrors"
    ]
  },
  "dependencies": {
    "FParsec": "1.0.2"
  },
  "tools": {
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": "dnxcore50"
    }
  },
  "frameworks": {
    "netstandard1.6": {
        "imports": "portable-net45+netcore45",
        "dependencies": {
             "Microsoft.FSharp.Core.netcore": "1.0.0-alpha-*"
        }
    },
    "net45" : {
      "dependencies": {
        "FSharp.Core" : "*"
      }
    }
  }
}